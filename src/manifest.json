{
  "name" : "Send to Readmill",
  "version" : "1.0",
  "description" : "Adds 'Send to Readmill' links to ePUBs",
  "update_url": "https://raw.github.com/matkins/send-to-readmill-chrome/master/updates.xml",
  "content_scripts" : [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["jquery-1.7.2.min.js","jquery.regex.js","contentscript.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 2
}